    "@mui/icons-material": "^5.14.14",
    "@mui/material": "^5.14.10",
    "@reduxjs/toolkit": "^1.9.7",
    "@types/node": "20.6.2",
    "@types/react": "18.2.22",
    "@types/react-dom": "18.2.7",
    "@types/react-redux": "^7.1.27",
    "eslint": "8.49.0",
    "eslint-config-next": "13.4.19",
    "next": "13.4.19",
    "next-cloudinary": "^4.22.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.47.0",
    "react-redux": "^8.1.3",
    "swiper": "^10.3.1",
